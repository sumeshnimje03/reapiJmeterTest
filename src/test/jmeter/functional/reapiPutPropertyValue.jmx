            <stringProp name="BeanShellAssertion.query">import com.eclipsesource.json.*;
import org.skyscreamer.jsonassert.*;

String jsonString = prev.getResponseDataAsString();
// Grab output JSON and print out as a string
JsonObject resultGroups = Json.parse(jsonString).asObject();
String resultText = resultGroups.toString();
log.info(&quot;Here is the output JSON: &quot; + resultText);

// JSON object for group1 
JsonObject errorDetail = new JsonObject();
errorDetail.add(&quot;jsonPath&quot;, &quot;properties/possibleValuesId&quot;);
errorDetail.add(&quot;details&quot;, &quot;Items must be unique&quot;);

//Json Array
JsonArray expectedData = new JsonArray();
expectedData.add(errorDetail);

// use put if add does not work
JsonObject expectedResult = new JsonObject();
expectedResult.add(&quot;type&quot;, &quot;DATA_VALIDATION_ERROR&quot;);
expectedResult.add(&quot;message&quot;, &quot;There are data validation errors in the form.&quot;);
expectedResult.add(&quot;errorDetails&quot;, expectedData);


log.info(&quot;Here is the expect JSON: &quot; + expectedResult.toString());
// Check for object equivalence
JSONCompareResult result = JSONCompare.compareJSON(expectedResult.toString(), resultText, JSONCompareMode.NON_EXTENSIBLE);
if (result.failed()) {
	Failure = true;
	FailureMessage = &quot;The result JSON does not match the expected JSON.\n&quot; + result.getMessage();
}</stringProp>
