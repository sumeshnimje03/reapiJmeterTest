String jsonString = prev.getResponseDataAsString();

// Grab output JSON and print out as a string

JsonObject resultEvents = Json.parse(jsonString).asObject();
String resultText = resultEvents.toString();
log.info(&quot;Here is the output JSON: &quot; + resultText);

// Build the expected JSON and print it out as a string

//Jason object for group1
JsonObject eventTimelineStats = new JsonObject();
eventTimelineStats.add(&quot;eventStatus&quot;, &quot;ACTIVE&quot;);
eventTimelineStats.add(&quot;terminatedMs&quot;, &quot;0&quot;);
eventTimelineStats.add(&quot;elapsedTimeInMs&quot;, &quot;${elapsedTimeInMs}&quot;);
eventTimelineStats.add(&quot;startDateTime&quot;, &quot;${startDateTime}&quot;);
eventTimelineStats.add(&quot;endDateTime&quot;, &quot;${endDateTime}&quot;);

//Jason object for group2
JsonObject eventUserDeliveryStats = new JsonObject();
eventUserDeliveryStats.add(&quot;eventStatus&quot;,&quot;ACTIVE&quot;);
eventUserDeliveryStats.add(&quot;totalUniqueUser&quot;,&quot;3&quot;);
eventUserDeliveryStats.add(&quot;uniqueUserPending&quot;,&quot;0&quot;);
eventUserDeliveryStats.add(&quot;uniqueUserDelivered&quot;,&quot;0&quot;);
eventUserDeliveryStats.add(&quot;uniqueUserResponded&quot;,&quot;3&quot;);
eventUserDeliveryStats.add(&quot;uniqueUserFailed&quot;,&quot;0&quot;);

//json object for group3
JsonObject eventResponseStats = new JsonObject();
//String[] obj = {&quot;No Response&quot;, &quot;stop Notifying user&quot;, &quot;Record response&quot;};
/*eventResponseStats.put(&quot;responses&quot;, (Object)obj);
eventResponseStats.add(&quot;responses&quot;,&quot;No Response&quot;);
eventResponseStats.add(&quot;responses&quot;,&quot;stop Notifying user&quot;);
eventResponseStats.add(&quot;responses&quot;,&quot;Record response&quot;);
**/
List responses = new ArrayList();
responses.add(&quot;No Response&quot;);
responses.add(&quot;stop Notifying user&quot;);
responses.add(&quot;Record response&quot;);
eventResponseStats.add(responses);
//eventResponseStats.add(&quot;counts&quot;,&quot;0&quot;);
//eventResponseStats.add(&quot;counts&quot;,&quot;0&quot;);
//eventResponseStats.add(&quot;counts&quot;,&quot;0&quot;);
